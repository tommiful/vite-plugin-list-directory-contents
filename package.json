{
  "name": "vite-plugin-list-directory-contents",
  "version": "1.1.1",
  "description": "A Vite plugin for listing the directory contents when working with the dev server",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "types": "dist/plugin.d.ts",
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./dist/plugin.js",
      "import": "./dist/plugin.mjs",
      "types": "./dist/plugin.d.ts"
    }
  },
  "scripts": {
    "build": "tsup plugin.ts --dts --format esm,cjs"
  },
  "author": "Wes Bos",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.8.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "vite": "^3.1.6"
  }
}
